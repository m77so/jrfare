{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": ["esnext","esnext.array"],
    "allowJs": true,
    "outDir": "./build",
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "sourceMap": true
  },
  "exclude": [
    "node_modules",
    "build",
    "scripts",
    "acceptance-tests",
    "webpack",
    "jest",
    "src/setupTests.ts",
    "data"
  ]
}
